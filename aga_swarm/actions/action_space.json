        "type": "python_script_call_main",
        "name": "file_retrieval.py",
        "id": "aga_swarm/actions/data/file_operations/file_retrieval/file_retrieval.py",
        "description": "This action calls the appropriate file retrieval action based on the 'platform' param in the swarm config",
        "input_schema": {
            "type": "object",
            "properties": {
                "file_path": {
                    "type": "string",
                    "description": "The path to the file to be retrieved"
                },
                "swarm": {
                    "type": "dict",
                    "description": "The swarm"
                }
            },
            "required": ["file_path"]
        },
        "output_schema": {
            "type": "object",
            "properties": {
                "status_message": {
                    "type": "string",
                    "description": "'Success' or 'Failure'"
                },
                "error_message": {
                    "type": "string",
                    "description": "Error message if status_message is 'Failure'"
                },
                "data": {
                    "type": "bytes",
                    "description": "The binary of the file"
                }
            },
            "required": ["status_message"]
        },
        "dependencies": [],
        "configs": []
    },
